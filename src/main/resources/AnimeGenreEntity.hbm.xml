<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="anime.parser.domain.AnimeGenreEntity" table="anime_genre" schema="" catalog="my_db">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="animesId">
            <column name="animes_id" sql-type="int" not-null="true"/>
        </property>
        <property name="genresId">
            <column name="genres_id" sql-type="int" not-null="true"/>
        </property>
        <many-to-one name="animesByAnimesId" class="anime.parser.domain.AnimesEntity">
            <column name="animes_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="genresByGenresId" class="anime.parser.domain.GenresEntity">
            <column name="genres_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>