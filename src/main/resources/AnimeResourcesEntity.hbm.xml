<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="anime.parser.domain.AnimeResourcesEntity" table="anime_resources" schema="" catalog="my_db">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="url">
            <column name="url" sql-type="varchar"/>
        </property>
        <many-to-one name="animesByAnimesId" class="anime.parser.domain.AnimesEntity">
            <column name="animes_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="resourcesNameByResourcesNameId" class="anime.parser.domain.ResourcesNameEntity">
            <column name="resources_name_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>